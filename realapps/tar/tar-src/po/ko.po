msgstr "POSIX 1003.1-1988 (ustar) 형식"

#: src/tar.c:696
msgid "POSIX 1003.1-2001 (pax) format"
msgstr "POSIX 1003.1-2001 (pax) 형식"

#: src/tar.c:697
msgid "same as pax"
msgstr "pax와 동일"

#: src/tar.c:701
msgid "same as --format=v7"
msgstr "--format=v7과 동일"

#: src/tar.c:704
msgid "same as --format=posix"
msgstr "--format=posix와 동일"

