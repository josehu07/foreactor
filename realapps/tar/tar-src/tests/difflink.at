AT_SETUP([link mismatch])
AT_KEYWORDS([diff difflink])
AT_TAR_CHECK([
mkdir a
genfile -f a/x
ln -s x a/y
ln a/x a/z
tar cf a.tar a/x a/y a/z
rm a/z
ln -s x a/z
tar df a.tar
],
[1],
[a/z: Not linked to a/x
],
[],
[],[],[ustar]) # Testing one format is enough
AT_CLEANUP

