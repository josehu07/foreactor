"  \\a      uyarı (BEL)\n"
"  \\b      geri sil\n"
"  \\c      artık çıktı gösterme\n"
"  \\e      kaçış\n"
"  \\f      form besleme\n"
"  \\n      yeni satır\n"
"  \\r      taşıyıcı döngüsü (CR)\n"
"  \\t      yatay sekme\n"
"  \\v      dikey sekme\n"

#: src/echo.c:78
